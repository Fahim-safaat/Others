#include<stdio.h>
int main(){
    int n1,n2;
 printf("Enter the size of A the matrix : ");
 scanf("%d %d", &n1,&n2);

 int a[100][100],i,j;

 //input

 for(i=0;i<n1;i++){
    for(j=0;j<n2;j++){
        printf("Enter the element of the A matrix [%d][%d]: ", i,j);
        scanf("%d", &a[i][j]);
    }
}
 //output
 printf("A Matrix is: \n");
 for(i=0;i<n1;i++){
    for(j=0;j<n2;j++){
        printf("%d  ", a[i][j]);
    }
    printf("\n");
}

//b matrix
int n3,n4;
 printf("Enter the size of the B matrix : ");
 scanf("%d %d", &n3,&n4);

 int b[100][100],k,l;

 //input

 for(k=0;k<n3;k++){
    for(l=0;l<n4;l++){
        printf("Enter the element of the B matrix [%d][%d]: ", k,l);
        scanf("%d", &b[k][l]);
    }
}
 //output
 printf("Matrix is : \n");
 for(k=0;k<n3;k++){
    for(l=0;l<n4;l++){
        printf("%d  ", b[k][l]);
    }
    printf("\n");
}

return 0;
}
