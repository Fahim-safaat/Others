#include<stdio.h>
int main()
{
    int arr1[100],arr2[100],n,i;

    printf("Enter how many value you want to store : ");
    scanf("%d", &n);

    for(i = 0; i<n; i++)
    {
        scanf("%d", &arr1[i]);
    }

    printf("Array1 is : ");
    for(i=0;i<n;i++){
        printf("%d", arr1[i]);
    }
    printf("\n");
    for(i=0;i<n;i++){
        arr2[i] = arr1[i];
    }
    printf("Array2 is : ");
    for(i=0;i<n;i++){
        printf("%d", arr2[i]);
    }

    return 0;
}

