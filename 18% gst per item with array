#include<stdio.h>
int main (){

float total, price[3];

printf("Enter price of three items: \n");
scanf("%f %f %f",&price[0],&price[1],&price[2]);

printf("Total price 1: %.2f\n", (price[0]+(price[0]*0.18)));
printf("Total price 2: %.2f\n", (price[1]+(price[1]*0.18)));
printf("Total price 1: %.2f\n", (price[2]+(price[2]*0.18)));

getch();
return 0;
}
